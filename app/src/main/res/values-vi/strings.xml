  <string name="new_name_upgrade_page_text">Với sản phẩm đã phát triển, chúng tôi mang đến cho bạn một %1$s mới mẻ, với tên mới và logo mới.</string>
  <string name="first_run_upgrade_page_text1">1. Đa nhiệm bằng cách mở %s cùng một lúc!</string>
  <string name="first_run_upgrade_page_text1_highlight">nhiều thẻ</string>
