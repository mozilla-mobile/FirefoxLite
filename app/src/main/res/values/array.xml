<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- data saving path entries -->
    <string-array name="data_saving_path_entries">
        <item>@string/setting_dialog_sd_card</item>
        <item>@string/setting_dialog_internal_storage</item>
    </string-array>

    <!-- data saving path values-->
    <!-- ordering of this array DOES matter, it effects Settings.shouldSaveToExternalStorage() -->
    <string-array name="data_saving_path_values">
        <item>@string/pref_value_saving_path_sd_card</item>
        <item>@string/pref_value_saving_path_internal_storage</item>
    </string-array>

    <!-- clean browsing data entries -->
    <string-array name="clean_browsing_data_entries">
        <item>@string/setting_dialog_browsing_history</item>
        <item>@string/setting_dialog_cookies</item>
        <item>@string/setting_dialog_cache</item>
    </string-array>

    <!-- clean browsing data values -->
    <string-array name="clean_browsing_data_values">
        <item>@string/pref_value_clear_browsing_history</item>
        <item>@string/pref_value_clear_cookies</item>
        <item>@string/pref_value_clear_cache</item>
    </string-array>
</resources>